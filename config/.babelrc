{
    "presets": [
        ["env", {
            "targets": {
                "node": "7"
            }
        }],
        "stage-2"
    ],

    "plugins": [
        "add-module-exports",
        [
            "module-resolver",
            {
                "root": ["./config/src/"],
                "alias": {
                    "~": "./config/src/"
                }
            }
        ]
    ]
}
